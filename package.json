{
  "name": "nodejs-website-boilerplate",
  "version": "1.0.0",
  "description": "nodejs website boilerplate",
  "main": "app.js",
  "scripts": {
    "development": "cross-env NODE_ENV=development PORT=5000 nodemon index.js -e js,html",
    "concat-js": "rm -f public/js/combined.js; rm -f public/js/combined.min.js; concat-cli -f public/js/* -o public/js/combined.js",
    "uglifyjs": "touch public/js/combined.min.js; uglifyjs public/js/combined.js -o public/js/combined.min.js",
    "less": "lessc public/less/main.less public/css/main.css",
    "dev": "cross-env NODE_ENV=development nodemon app.js",
    "start": "cross-env NODE_ENV=production node -r @babel/register app.js",
    "concat-css": "concat-cli -f public/css/font-awesome.min.css public/css/css-helpers.min.css public/css/combined.css -o public/css/combined.css",
    "minify-css": "touch public/css/combined.min.css; cleancss -o public/css/combined.min.css public/css/combined.css"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "async": "^2.0.0",
    "axios": "^1.7.7",
    "body-parser": "^1.15.2",
    "clean-css": "^5.3.3",
    "compression": "^1.6.1",
    "concat-cli": "^4.0.0",
    "cosmicjs": "^5.0.2",
    "express": "^4.21.0",
    "hogan-express": "^0.5.2",
    "install": "^0.4.4",
    "less": "^2.7.1",
    "lodash": "^4.13.1",
    "mailgun-js": "^0.6.7",
    "npm": "^10.9.0",
    "uglify-js": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/register": "^7.25.7",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7"
  }
}
